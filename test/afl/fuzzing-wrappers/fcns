
TOP_DIR="fuzzing-output"

SERVER="../../server/.libs/fwknopd"
CLIENT="../../client/.libs/fwknop"
LIB_DIR="../../lib/.libs"

git_banner() {
    if [ -d '../../.git' ] && [ `which git` ]
    then
        C=`git log --abbrev-commit | head -n 1 | awk '{print $2}'`
        eval "$1=,$C"
    fi
}

dir_init() {
    [ ! -d $1 ] && mkdir -p $1
    TS=`date +"%m%d%y%H%M%S"`
    [ -d $3 ] && mv $3 "$1/$2-$TS"
    mkdir $3
}
